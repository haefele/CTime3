    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch Command-Line App",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/apps/CTime3.Apps.CommandLine/bin/Debug/net6.0/CTime3.Apps.CommandLine.dll",
            "args": ["login"],
            "cwd": "${workspaceFolder}/apps/CTime3.Apps.CommandLine",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false
        }
    ]
}